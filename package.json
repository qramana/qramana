{
  "name": "@qramana/qramana",
  "version": "0.0.1",
  "description": "TypeScript library for quantum calculation",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p ./",
    "build:example": "tsc -p ./example",
    "test": "npm run build && npm run test:build && npm run test:jest && npm run lint",
    "test:build": "tsc -p ./spec",
    "test:jest": "jest",
    "lint": "npm run lint:src && npm run lint:spec",
    "lint:src": "tslint -c tslint.json src/*.ts src/**/*.ts --project ./tsconfig.json",
    "lint:spec": "tslint -c tslint.json spec/*.ts spec/**/*.ts --project ./spec/tsconfig.json"
  },
  "keywords": [
    "quantum"
  ],
  "repository": {
    "type": "git",
    "url": "qramana/qramana.git"
  },
  "publishConfig": {
    "access": "public",
    "@qramana:registry": "https://registry.npmjs.org/"
  },
  "author": "Qramana Team (https://github.com/qramana)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/jsqubits": "1.1.0",
    "jest": "^24.1.0",
    "tslint": "^5.4.3",
    "typescript": "^3.0.0"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "jsqubits": "^1.1.1"
  }
}
